  "license": "ISC",
  "devDependencies": {
    "ava": "^0.12.0",
    "mongoose": "^4.4.6",
    "nyc": "^6.2.1",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "co": "^4.6.0",
    "farmhash": "^1.1.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelkrone/monrule.git"
  },
  "bugs": {
    "url": "https://github.com/michaelkrone/monrule/issues"
