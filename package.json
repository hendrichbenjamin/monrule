{
  "name": "monrule",
  "version": "0.0.2",
  "description": "A simple cache to persist expensive function results",
  "main": "./lib/index.js",
  "scripts": {
    "test": "nyc ./node_modules/ava/cli.js",
    "watch": "node ./node_modules/ava/cli.js --watch",
    "report": "nyc ./node_modules/ava/cli.js --reporter=html"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "failFast": true
  },
